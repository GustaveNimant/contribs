-- Le Gouvernement détermine et conduit la politique de la nation.

-- Il dispose de l'administration et de la force armée.

-- Il est responsable devant le Parlement dans les conditions et suivant les procédures prévues aux articles 49 et 50.

species Article_20_S (
  URac is Une_Race_S,
  UOri is Une_Origine_S,
  URel is Une_Religion_S,
  UTle is Un_Texte_legislatif_S,
  UOrd is Une_Ordonnance_S,
  UDec is Un_Decret_S,
  LAmi is L_Administration_S,
  LFar is Les_Forces_armees_S,
  UAex is Un_Acte_de_l_executif_S,
  LPna is La_Politique_de_la_nation_S (UAex),
  UTer is Un_Territoire_S,
  UScr is Un_Scrutin_S,
  UBvo is Un_Bureau_de_vote_S (UTer),
  UOde is Un_Organe_deliberant_S (UTle),
  UCel is Un_Citoyen_electeur_S (URac, UOri, URel, UTer, UScr, UBvo),
  UDep is Un_Depute_S (URac, UOri, URel, UTer, UScr, UBvo),
  USen is Un_Senateur_S (URac, UOri, URel, UTer, UScr, UBvo),
  UPar is Un_Parlementaire_S (URac, UOri, URel, UTer, UScr, UBvo),
  LEle is L_Electorat_S (URac, UOri, URel, UTer, UScr, UBvo, UCel),
  UElu is Un_Elu_S (URac, UOri, URel, UTer, UScr, UBvo),
  LAna is L_Assemblee_nationale_S (URac, UOri, URel, UTer, UScr, UBvo, UDep, UPar),
  LSen is Le_Senat_S (URac, UOri, URel, UTer, UScr, UBvo, USen, UPar),
  LPar is Le_Parlement_S (URac, UOri, URel, UTer, UScr, UBvo, UDep, USen, UElu, UPar, LAna, LSen),
  UMgo is Un_Membre_du_gouvernement_S (URac, UOri, URel, UTer, UScr, UBvo),
  LGou is Le_Gouvernement_S (URac, UOri, URel, UTle, UOrd, UDec, UTer, UScr, UBvo, UOde, UMgo)
  ) =

-- Titre III Article 20-a-1 - Le Gouvernement détermine la politique de la nation.

  signature le_gouvernement_determine_la_politique_de_la_nation : LGou -> LPna -> bool;

-- Titre III Article 20-a-2 - Le Gouvernement conduit la politique de la nation.

  signature le_gouvernement_conduit_la_politique_de_la_nation : LGou -> LPna -> bool;

-- Titre III Article 20-b-1 - Il dispose de l'administration

  signature le_gouvernement_dispose_de_l_administration : LGou -> LAmi -> bool;

-- Titre III Article 20-b-2 - Il dispose de la force armée.

  signature le_gouvernement_dispose_de_la_force_armee : LGou -> LFar -> bool;

-- Titre III Article 20-c - Il est responsable devant le Parlement dans les conditions et suivant les procédures prévues aux articles 49 et 50.

  signature le_gouvernement_est_responsable_devant_le_parlement : LGou -> LPar -> bool;
end;;