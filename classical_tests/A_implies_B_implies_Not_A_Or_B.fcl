open "basics";;

theorem A_implies_B_implies_Not_A_Or_B :
  all a b : bool, (a -> b) -> (~ a \/ b) 
  proof = 
    <1>1 assume a : bool, b : bool,
         hypothesis h : (a -> b),
         prove (~ a \/ b) 
         by hypothesis h
    <1>2 conclude
;;