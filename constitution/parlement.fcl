open "basics";;
open "parlementaire";;
open "un_scrutin";;

species Parlement_S (S is Un_Scrutin_S, P is Un_Parlementaire_S (S)) = 

  representation = list (P);

  property parlementaire_redige_la_loi : all p : P, P!redige_la_loi(p);

  property parlement_est_souverain : all p : P, P!exerce_souverainete (p);

end;;