open "basics";;
open "un_parlementaire";;
open "un_scrutin";;
open "un_acte_de_l_executif";;
open "une_loi";;

species Un_Depute_S (USc is Un_Scrutin_S, 
                     UAe is Un_Acte_de_l_executif_S,
                     ULo is Une_Loi_S
                     ) = 

  inherit Un_Parlementaire_S (USc, UAe, ULo); 

  property est_elu : all p : Self, all s : USc,
        est_elu_par_le_scrutin (p, s) ->
        USc!est_une_election_legislative (s);

end;;